@model List<UserDTO>
@{
}

@if (Model.Count > 0)
{
    @foreach (var user in Model)
    {
        <a asp-action="UserScreen" asp-controller="Profile" asp-route-Id="@user.Id" style="background-image: url(/Imagens/UsersBackground/@user.BackgroundUrl);" class="userElement">
            <img src="~/Imagens/UsersImg/@user.ImgUrl" alt="">
            <span>@user.UserName</span>
        </a>
    }

}
else
{
    <h2>Não Encontrado</h2>
}

